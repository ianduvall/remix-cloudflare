{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build:app": "remix build",
    "build:css": "npm run generate:css -- --minify",
    "build": "run-s build:css build:app",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public",
    "dev": "run-p dev:*",
    "format": "prettier . --cache --write",
    "generate:css": "tailwindcss -i styles/root.css -o app/styles/root-generated.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^0.7.2",
    "@remix-run/cloudflare": "^1.6.0",
    "@remix-run/cloudflare-pages": "^1.6.0",
    "@remix-run/react": "^1.6.0",
    "cross-env": "^7.0.3",
    "framer-motion": "^6.3.11",
    "react": "^18.2.0",
    "react-a11y-dialog": "^6.1.5",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^1.3.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.12.0",
    "@remix-run/dev": "^1.6.0",
    "@remix-run/eslint-config": "^1.6.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.2",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.11",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.0",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "tailwindcss": "^3.1.3",
    "tailwindcss-aria-attributes": "^1.3.0",
    "typescript": "^4.7.3",
    "wrangler": "beta"
  },
  "engines": {
    "node": ">=16"
  }
}
